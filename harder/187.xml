<?xml version="1.0" encoding="UTF-8"?>
<formula id="10">
<input id="num1" type="bool" label="无咳嗽"/>
<input id="num2" type="bool" label="颈前淋巴结肿大/触痛"/>
<input id="num3" type="bool" label="体温超过38℃"/>
<input id="num4" type="bool" label="扁桃体肿胀或有渗出液"/>
<input id="num5" type="bool" label="3~14岁"/>
<input id="num6" type="bool" label="15~44岁"/>
<input id="num7" type="bool" label="≥45岁"/>
<result id="score" type="score" label="结果"  hidden="yes">
num1 + num2  + num3  + num4  + num5  + num6  undefined num7 
</result>
<interpretation for="score">
										<text range=">=1">患者有较高风险发生严重不良转归</text>
										<text range="<1">患者有较低风险发生严重不良转归</text>
								</interpretation>
</formula>
